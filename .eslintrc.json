{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "rules": {
    "prettier/prettier": ["error"],
    "import/no-dynamic-require": 0,
    "array-callback-return": 0,
    "consistent-return": 0,
    "no-param-reassign": 0
  }
}
